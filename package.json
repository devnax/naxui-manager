{
    "version": "1.4.4",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "description": "The naxui-manager is an innovative React library that revolutionizes the process of creating custom UI library",
    "files": [
        "dist",
        "src"
    ],
    "keywords": [
        "react-ui",
        "react ui builder",
        "react custom ui manager"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "naxui-manager",
    "author": "Naxrul Ahmed",
    "module": "dist/naxui-manager.esm.js",
    "size-limit": [
        {
            "path": "dist/naxui-manager.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/naxui-manager.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.2.6",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "husky": "^8.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "size-limit": "^8.2.6",
        "tsdx": "^0.14.1",
        "tslib": "^2.6.0",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "naxcss": "^2.4.7"
    }
}
